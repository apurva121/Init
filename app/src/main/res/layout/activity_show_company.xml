<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.practice.mnnit.init.showCompany">

    <android.support.v7.widget.Toolbar
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="#3F51B5"
        app:theme="@style/ThemeOverlay.AppCompat.Dark"
        android:id="@+id/showCompanyToolbar">

    </android.support.v7.widget.Toolbar>



    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView5"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/showCompanyToolbar">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/showCompanyName1"
                android:clickable="false"
                android:background="#000000"
                android:textColor="#FFFFFF"
                android:textAppearance="?android:attr/textAppearanceMedium"
                />


            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/showCompanyName2"
                android:layout_below="@+id/showCompanyName1"
                android:textColor="#FFFFFF"
                android:background="#000000"
                android:clickable="true"
                android:textAppearance="?android:attr/textAppearanceMedium"
                />

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/showCompanyName3"
                android:layout_below="@+id/showCompanyName2"
                android:background="#000000"
                android:textColor="#FFFFFF"
                android:clickable="false"
                android:textAppearance="?android:attr/textAppearanceMedium"
                />



            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/showCompanyName4"
                android:layout_below="@+id/showCompanyName3"
                android:textColor="#FFFFFF"
                android:background="#000000"
                android:clickable="false"
                android:textAppearance="?android:attr/textAppearanceMedium"
                />



            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/showCompanyName5"
                android:layout_below="@+id/showCompanyName4"
                android:textColor="#FFFFFF"
                android:background="#000000"
                android:clickable="false"
                android:textAppearance="?android:attr/textAppearanceMedium"
                />


            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/showCompanyName6"
                android:layout_below="@+id/showCompanyName5"
                android:textColor="#FFFFFF"
                android:background="#000000"
                android:clickable="false"
                android:textAppearance="?android:attr/textAppearanceMedium"
                />


            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/showCompanyName7"
                android:layout_below="@+id/showCompanyName6"
                android:background="#000000"
                android:textColor="#FFFFFF"
                android:clickable="false"
                android:textAppearance="?android:attr/textAppearanceMedium"
                />



            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/showCompanyName8"
                android:layout_below="@+id/showCompanyName7"
                android:textColor="#FFFFFF"
                android:background="#000000"
                android:clickable="false"
                android:textAppearance="?android:attr/textAppearanceMedium"
                />



            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/showCompanyName9"
                android:layout_below="@+id/showCompanyName8"
                android:background="#000000"
                android:textColor="#FFFFFF"
                android:clickable="false"
                android:textAppearance="?android:attr/textAppearanceMedium"
                />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Register"
                android:clickable="false"
                android:background="#000000"
                android:textColor="#FFFFFF"
                android:id="@+id/button17"
                android:layout_below="@+id/showCompanyName9"
                android:layout_centerHorizontal="true"/>

            <WebView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/webView"
                android:visibility="invisible"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true" />
        </RelativeLayout>
    </ScrollView>



</RelativeLayout>
